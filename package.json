    "build": "npm run build:ci",
    "build:debug": "mkdir -p report && webpack --config webpack/webpack.prod-debug.config.js --profile --json | sed '1d' | tee ./report/stat.json | webpack-bundle-analyzer /dev/stdin ./build -s gzip -l debug",
    "build:ci": "webpack --config webpack/webpack.prod-ci.config.js",
    "build:ci-report": "mkdir -p report && webpack --config webpack/webpack.prod-ci.config.js --profile --json | tee ./report/stat.json | webpack-bundle-analyzer /dev/stdin ./build -s gzip -l debug",
